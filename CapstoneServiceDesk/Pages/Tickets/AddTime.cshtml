
@{
    ViewData["Title"] = "Adding Time For " + Model.TicketTitle;
}
@using CapstoneServiceDesk.Models.TicketsViewModels
@model TicketTime

<form method="post">
    <input asp-for="TicketId" type="hidden">
    <input asp-for="StartTime">
    <input asp-for="EndTime">
    <button class="btn" type="submit">
        Add Time
    </button>
</form>